{
    "compilerOptions": {
        "noImplicitAny": false,
        "module": "esNext",
        "moduleResolution": "node",
        "target": "es5",
        "jsx": "react",
        "sourceMap": true,
        "declaration": true,
        "baseUrl": "./src",
        "outDir": "src/main.g",
        "lib": ["dom", "es2016", "es2017.object"],
        "paths": {
            "app/*": ["App/*"],
            "store/*": ["App/store/*"],
            "proxy/*": ["App/proxy/*"],
            "components/*": ["App/components/*"],
            "mviews/*": ["App/Views/Multiply/*"],
            "sviews/*": ["App/Views/Single/*"],
            "pviews/*": ["App/Views/Print/*"],
            "routes/*": ["App/routes/*"],
            // "reg/*": ["App/reg/Registration/*"],
            "base/*": ["main/base/*"],
            "routes": ["App/routes"],
        }
    },
}
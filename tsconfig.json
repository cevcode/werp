{
    "compilerOptions": {
        "noImplicitAny": false,
        "module": "esNext",
        "moduleResolution": "node",
        "target": "es5",
        "jsx": "react",
        "sourceMap": true,
        "declaration": true,
        "baseUrl": "./src",
        "outDir": "src/main.g",
        "lib": ["dom", "es2016", "es2017.object"],
        "paths": {
            "store/*": ["app/store/*"],
            "proxy/*": ["app/proxy/*"],
           "components/*": ["app/components/*"],
        }
    },
}